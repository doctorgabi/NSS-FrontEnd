extends ../template/layout

block content
  .row
    .small-4.columns.small-offset-4.columns
      .headerWrapper
        h1.subheader Artists Page
  .row
    .small-12.columns
      #artists
        each artist in artists
          .artist.float
            h3= artist.name
            a(href="/artists/#{artist.id}")
              img(src=artist.photo)
        .clear
